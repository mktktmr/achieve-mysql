<div class="container">
  <h2>ユーザー</h2>
  <% @users.each do |user| %>
    <div class="container-child col-md-2 col-sm-4 col-xs-6 ">
      <a href="<%= user_path user %>">
        <%= profile_img(user) %>
        <%= user.name %>
      </a>
      <% if user.id != current_user.id%>
        <%= render 'follow_form', user: user %>
      <% end %>
    </div>
  <% end %>
</div>
